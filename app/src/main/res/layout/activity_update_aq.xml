        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.499" />

    <EditText
        android:id="@+id/Add"
        android:layout_width="374dp"
        android:layout_height="64dp"
        android:background="@drawable/coustomtext1"
